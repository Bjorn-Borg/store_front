{
  "name": "store_front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:start": "docker-compose up",
    "dev:stop": "docker-compose stop storefront",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "performance:show": "lighthouse http://localhost:3000 --quiet --chrome-flags='--headless' --view",
    "performance": "bash ./scripts/lighthouse_runner",
    "test": "jest --watchAll",
    "dev:test": "docker exec storefront /bin/bash -c 'jest --watchAll'",
    "test:ci": "jest --ci",
    "test:pc": "bash ./scripts/jest_hook",
    "lint": "docker exec storefront /bin/bash -c 'eslint . --ext .tsx,.ts --fix --max-warnings 10'"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/types": "^5.0.0",
    "csv-writer": "^1.6.0",
    "next": "9.3.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "eslint-plugin-jest": "^23.8.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.5",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "react-test-renderer": "^16.12.0",
    "typescript": "^3.8.3"
  }
}
